<div class="form--contained">
	<div class="as--admin-index-title">
		<h1>Editing Header</h1>
		<%= link_to 'Back', admin_show_path, class: 'button button--primary button--large as--no-margin-bottom' %>
	</div>
	<div class="admin--module">
		<p>Recent Updates</p>
		<% @updated_by.each do |update|%>
			<li><%= update.to_h[:user].email%> | <%= update.to_h[:time]%></li>
		<% end %>
	</div>
	<%= render 'form' %>
</div>

